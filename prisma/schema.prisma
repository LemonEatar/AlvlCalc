// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Test{
  id Int @id @default(autoincrement())
  title String
  year Int
  User String
}

model TimeTable{
  id Int @id @default(autoincrement())
  Monday String
  Tuesday String
  Wendsday String
  Thursday String
  Friday String
}
